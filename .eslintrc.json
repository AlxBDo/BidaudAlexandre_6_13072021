{
    "root": true,
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "es6": true
  },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["error", "double"]
    },
        "extends": [
            "eslint:recommended"
          ]
}
